{
  "name": "domic-app",
  "version": "0.1.1",
  "repository": "https://github.com/ceymard/domic-state",
  "description": "A client-side application state machine for Carbyne.",
  "license": "MIT",
  "main": "./src/index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "build:dts": "dts-bundle --name domic-app --main src/index.d.ts --out ../index.d.ts && sed -i '/\\(require\\|import\\).*\\.\\(styl\\|css\\)/d' ./index.d.ts",
    "build:js": "tsc",
    "build": "npm run build:js && npm run build:dts",
    "prepublish": "npm run build",
    "watch": "parallel -u ::: 'ls src/*.d.ts | entr -d npm run build:dts' 'tscc -w'"
  },
  "dependencies": {
    "domic": "*"
  },
  "devDependencies": {
    "@types/es6-collections": "0.0.28",
    "@types/es6-promise": "0.0.29",
    "dts-bundle": "^0.5.0"
  }
}
